{
  "service": {
    "id": "{{ inventory_hostname }}-node-exporter",
    "name": "node-exporter",
    "tags": ["node-exporter", "metrics"],
    "port": 9100,
    "check": {
      "id": "node-exporter-api",
      "name": "node-exporter endpoint on port 9100",
